# Ignore bundler-related files
/.bundle
/vendor/bundle
/log/*
/tmp/*
/node_modules
/yarn-error.log
/package-lock.json
/public/assets
/public/packs
/public/packs-test
/storage/*
!/storage/.keep

# Ignore database and credentials
/config/master.key
/config/credentials.yml.enc
/db/*.sqlite3
/db/*.sqlite3-journal
/db/schema.rb
/db/development.sqlite3
/db/development.sqlite3-journal
/db/test.sqlite3
/db/test.sqlite3-journal

# Ignore system files
*.rbc
*.sass-cache
*.swp
*.swo
.DS_Store
.idea
.vscode
.byebug_history
/log/development.log
/log/test.log

# Ignore coverage reports
/coverage/
/capybara-*.html

# Ignore compiled files
/public/system
/public/uploads
